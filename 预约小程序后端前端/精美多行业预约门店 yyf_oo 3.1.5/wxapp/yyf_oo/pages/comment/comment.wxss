page {
    background: #f7faf8;
}

.layout {
    width: 95%;
    background: white;
    min-height: 500px;
    margin: 0px auto;
}

.blank_div {
    width: 100%;
    height: 35rpx;
}

.star-image {
    position: absolute;
    margin-left: 100rpx;
    width: 100rpx;
    height: 100rpx;
    src: "../../resource/images/no-star.png";
}

.item {
    position: absolute;
    top: 0rpx;
    width: 100rpx;
    height: 100rpx;
}

.star_div {
    width: 100%;
    height: 130rpx;
}

.content_div {
    width: 90%;
    height: 200rpx;
    margin: 0 auto;
}

.content {
    width: 100%;
    background: #fafafa;
    border: 1px solid #cccccc;
    font-size: 14px;
}

.pfont {
    font-size: 14px;
    height: 30px;
    line-height: 30px;
    text-indent: 10rpx;
}

.submit_btn {
    width: 90%;
    margin: 150rpx auto;
}